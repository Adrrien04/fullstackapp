{
  "name": "fullstackapp",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"node server.js\"",
    "test": "jest",
    "dev:vite": "vite --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.1",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "bootstrap": "^5.3.3",
    "bootstrap-vue": "^2.23.1",
    "bootstrap-vue-3": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "jquery": "^3.7.1",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.2",
    "koa-bodyparser": "^4.4.1",
    "koa-send": "^5.0.1",
    "mdb-vue-ui-kit": "^4.1.1",
    "mongoose": "^8.2.3",
    "popper.js": "^1.16.1",
    "swiper": "^8.4.7",
    "vue": "^3.4.21",
    "vue-awesome-swiper": "^5.0.1",
    "vue-router": "^4.3.0",
    "vue3-datepicker": "^0.4.0",
    "vue3-google-login": "^2.0.26",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.0-alpha.2",
    "babel-jest": "^26.6.3",
    "chai": "^4.2.0",
    "chai-http": "^4.4.0",
    "concurrently": "^8.2.2",
    "jest": "^26.6.3",
    "jsdom": "^24.0.0",
    "jsdom-global": "^3.0.2",
    "vite": "^5.1.6",
    "vue-jest": "^5.0.0-alpha.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "mjs",
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.mjs$": "babel-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    }
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!axios)",
    "/node_modules/(?!chai)"
  ],
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1"
  }
}
